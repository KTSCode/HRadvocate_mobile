{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
      "react",
      "react-native",
      "prettier"
    ],
    "env": {
      "react-native/react-native": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
   "rules": {
    "react-native/no-inline-styles": 2
    }
}
